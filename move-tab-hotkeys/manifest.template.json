{
    "name": "Move Tab Hotkeys${TITLE_EXTRA}",
    "description": "Hotkeys to move tabs around",
    "homepage_url": "https://github.com/jmmerz/move-tab-hotkeys",
    "manifest_version": 3,
    "version": "2.3.0",
    ${APPLICATIONS}
    "author": "Jonathon Merz",
    "icons": {
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
